.slot {
    width: 30px;
    height: 30px;
    background-color: rgb(255, 255, 255);
    margin: 5px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.slot_row {
    flex-direction: row;
}

.object {
    width: 20px;
    height: 20px;
    position: absolute;
    left: 10px;
    top: 10px;
}

.br {
    width: 30px;
    height: 30px;
}

.tile {
    width: 30px;
    height: 30px;
    background-color: rgb(255, 255, 255);
    margin: 5px;
}
